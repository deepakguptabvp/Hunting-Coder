{
    "version": 2,
    "builds": [
        {
            "src": "src/app/page.js",
            "use": "vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest" :"src/app/page.js",
            "methods":["GET", "POST","PUT","DELETE","OPTIONS"],
            "headers":{ "Access-Control-Allow-Origin" :"*"
            }
        }
    ]
}